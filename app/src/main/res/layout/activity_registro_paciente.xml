<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout  xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".administrador.RegistroPacienteActivity"
    android:background="@drawable/fondoregistro">

    <Button
        android:id="@+id/buttonRegistoBD"
        android:layout_width="132dp"
        android:layout_height="55dp"
        android:layout_marginBottom="68dp"
        android:background="@drawable/buttonlogin"
        android:text="Aceptar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.422"
        app:layout_constraintStart_toEndOf="@+id/buttonCancelarReg" />

    <Button
        android:id="@+id/buttonCancelarReg"
        android:layout_width="132dp"
        android:layout_height="55dp"
        android:layout_marginBottom="68dp"
        android:background="@drawable/buttonlogin2"
        android:text="Cancelar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.204"
        app:layout_constraintStart_toStartOf="parent" />

    <Spinner
        android:id="@+id/spinnerEnfermedad"
        android:layout_width="302dp"
        android:layout_height="54dp"
        android:layout_margin="100dp"
        android:background="@drawable/textgap"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.864" />

    <Spinner
        android:id="@+id/spinnerMedico"
        android:layout_width="302dp"
        android:layout_height="54dp"
        android:layout_margin="100dp"
        android:layout_marginBottom="120dp"
        android:background="@drawable/textgap"
        app:layout_constraintBottom_toBottomOf="@+id/spinnerEnfermedad"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTutor" />

    <EditText
        android:id="@+id/editTextNombreR"
        android:layout_width="302dp"
        android:layout_height="54dp"
        android:background="@drawable/textgap"
        android:ems="10"
        android:fontFamily="@font/poppins"
        android:gravity="left|center"
        android:hint="Nombre"
        android:inputType="text"
        android:paddingLeft="15dp"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <EditText
        android:id="@+id/editTextFechaNacR"
        android:layout_width="302dp"
        android:layout_height="54dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/textgap"
        android:ems="10"
        android:fontFamily="@font/poppins"
        android:gravity="left|center"
        android:hint="Fecha"
        android:inputType="date"
        android:paddingLeft="15dp"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.522"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextApellidosR"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:fontFamily="@font/poppins"
        android:text="DATOS DEL PACIENTE"
        android:textColor="#260F5C"
        android:textSize="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextApellidosR"
        android:layout_width="302dp"
        android:layout_height="54dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/textgap"
        android:ems="10"
        android:fontFamily="@font/poppins"
        android:gravity="left|center"
        android:hint="Apellidos"
        android:inputType="text"
        android:paddingLeft="15dp"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.522"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextNombreR"
        app:layout_constraintVertical_bias="0.006" />

    <EditText
        android:id="@+id/editTextTutor"
        android:layout_width="302dp"
        android:layout_height="54dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/textgap"
        android:ems="10"
        android:fontFamily="@font/poppins"
        android:gravity="left|center"
        android:hint="Nombre completo tutor legal"
        android:inputType="text"
        android:paddingLeft="15dp"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.522"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintTop_toBottomOf="@+id/editTextFechaNacR" />






    <EditText
        android:id="@+id/editTextFto"
        android:layout_width="302dp"
        android:layout_height="54dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="text"
        android:hint="URL con la foto del paciente"
        android:background="@drawable/textgap"
        android:fontFamily="@font/poppins"
        android:gravity="left|center"
        android:paddingLeft="15dp"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.522"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTutor"
        app:layout_constraintVertical_bias="0.0" />


</androidx.constraintlayout.widget.ConstraintLayout>